<%=form_for(@post) do |f|%>

    <% render 'shared/error_messages'%>
    
    <%= f.label :title %><br>
    <%= f.text_field :title, placeholder: "Type a post title here" %><br>
    <%= f.label :body %><br>
    <%= f.text_area :body, size: "60x12", "What is on your mind?" %><br>

    <%= f.submit %>
<%end%>